    "editor.findRangeHighlightBackground": "#3c3836",
    "editor.findRangeHighlightBorder": "#3c3836",
    "editor.hoverHighlightBackground": "#3c3836",
    "editor.hoverHighlightBorder": "#3c3836",
    "editor.wordHighlightBackground": "#3c3836",
