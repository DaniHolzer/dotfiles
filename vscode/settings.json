    "editor.wordHighlightStrongBorder": "#3c3836",
    "editorBracketMatch.background": "#3c3836",
    "editorBracketMatch.border": "#3c3836",
    "editorCodeLens.foreground": "#3c3836",
    "editorError.foreground": "#3c3836",
    "editorError.border": "#3c3836",
    "editorWarning.foreground": "#3c3836",
    "editorWarning.border": "#3c3836",
    "editorInfo.foreground": "#3c3836",
    "editorInfo.border": "#3c3836",
    "editorGutter.background": "#191B1C",
    "editorGutter.modifiedBackground": "#3c3836",
    "editorGutter.addedBackground": "#3c3836",
    "editorGutter.deletedBackground": "#3c3836",
    "editorOverviewRuler.border": "#3c3836",
    "editorOverviewRuler.findMatchForeground": "#3c3836",
    "editorActiveLine": "#ED3276"

  },
  "editor.colorDecorators": false,

  "vim": {
    "neovimPath": "/opt/homebrew/bin/nvim",
    "useSystemClipboard": true
  }
}
